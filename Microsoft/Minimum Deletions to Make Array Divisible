from typing import *


class Solution:

    def findgcd(self,x,y) -> int:
        if y == 0:
            return x
        else:
            return self.findgcd(y,x%y)

    def gcd(self,lst:List) -> int:
        temp = 0
        for i in lst:
            temp = self.findgcd(temp,i)
        return temp


    def minOperations(self, nums: List[int], numsDivide: List[int]) -> int:
        minNum = self.gcd(numsDivide)
        print(minNum)
        nums.sort()

        ind = 0

        while ind < len(nums):
            if minNum % nums[ind] != 0:
                ind+=1
            else:
                return ind

        return -1
